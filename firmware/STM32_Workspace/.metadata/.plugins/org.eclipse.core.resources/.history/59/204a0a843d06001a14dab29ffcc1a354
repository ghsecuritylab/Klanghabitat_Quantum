typedef enum
{
	bypass=0,
	activate,
}Bypass;

void BypassActivate(Bypass State);
/*******************************************/
typedef enum
{
	MS=0,
	RL,
}Source;

void VCASource(Source State);
/*******************************************/
typedef enum
{
	Normal=0,
	MonoS,
	MonoM,
}Output;

typedef enum
{
	Active=0,
	Inactive,
}Convert;

void VCAOutput(Output Out, Convert Conv);
