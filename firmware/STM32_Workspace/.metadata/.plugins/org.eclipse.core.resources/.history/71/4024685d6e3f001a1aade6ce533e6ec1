typedef struct osc_message {
  char *format;  // a pointer to the format field
  char *adress;  // the current read head
  int32_t *integer;  // the original message data (also points to the address)
  float *number;
  uint32_t len;  // length of the buffer data
} osc_message;



int tosc_parseMessage(osc_message *o, char *buffer, const int len);
