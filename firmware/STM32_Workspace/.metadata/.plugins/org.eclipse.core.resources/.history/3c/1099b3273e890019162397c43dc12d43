/*********************************************************************
*
*       _cbDialog
*/
static void _cbDialog(WM_MESSAGE * pMsg) {
  const void * pData;
  WM_HWIN      hItem;
  U32          FileSize;
  int          NCode;
  int          Id;
  // USER START (Optionally insert additional variables)
  // USER END

/*
  i++;
if(i==100 || i == 0){
	i=0;
	add = add * (-1);
}

level = level + add;


*/
/*
